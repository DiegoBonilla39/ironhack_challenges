   "source": [
    "def strong_num(number):\n",
    "    import math\n",
    "    factorials = []\n",
    "\n",
    "    for i in str(number):\n",
    "        factorials.append(math.factorial(int(i)))\n",
    "\n",
    "    if sum(factorials) == number:\n",
    "        return(\"STRONG!!!!\")\n",
    "    else:\n",
    "        return(\"Not Strong !!\")\n",
    "\n"
   ]
